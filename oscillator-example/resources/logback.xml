<configuration>
    <contextName>oscillator-example</contextName>


    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <append>true</append>
        <encoder>
            <pattern>%date{HH:mm:ss.SSS} [%thread] %-5level %logger{15} - %m%n</pattern>
        </encoder>
    </appender>



    <root level="INFO">

        <appender-ref ref="console"/>
    </root>

</configuration>